@model IEnumerable<string>
@{
    if (Model != null)
    {
        <h1 class="bg-success text-white">Информация входа</h1>

        <table class="table table-sm table-bordered">
            <tr><th>Name</th><th>Email</th></tr>
            <tr>
                @foreach (string info in Model)
                {
                    <td>@info</td>
                }

            </tr>
        </table>
    }
    else
    {
        <h1 class="bg-danger text-white"> Ошибка входа</h1>

    }
}
